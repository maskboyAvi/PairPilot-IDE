# Required (Supabase)
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# Optional (Upstash rate limiting)
UPSTASH_REDIS_REST_URL=
UPSTASH_REDIS_REST_TOKEN=

# Optional (Sentry)
# Browser DSN MUST be NEXT_PUBLIC_*
NEXT_PUBLIC_SENTRY_DSN=
# Server/edge DSN (can be same as public DSN)
SENTRY_DSN=

# Optional (Sentry sourcemaps upload during build)
# Only needed if you want readable stack traces in production.
SENTRY_AUTH_TOKEN=
SENTRY_ORG=
SENTRY_PROJECT=